# Environment variables for Render deployment
# Copy these to your Render environment variables section

# Required for ElizaOS
OPENAI_API_KEY=your_openai_api_key_here

# Required for Discord integration
DISCORD_APPLICATION_ID=your_discord_application_id_here
DISCORD_API_TOKEN=your_discord_token_here

# Required for bot operation
DAEMON_PROCESS=true
NODE_ENV=production

# Optional Farcaster integration
FARCASTER_NEYNAR_API_KEY=your_farcaster_api_key_here
FARCASTER_SIGNER_UUID=your_farcaster_signer_uuid_here
FARCASTER_FID=your_farcaster_fid_here
FARCASTER_DRY_RUN=true
ENABLE_CAST=true
CAST_INTERVAL_MIN=60
CAST_INTERVAL_MAX=120
ENABLE_ACTION_PROCESSING=true
ACTION_INTERVAL=60

# Logging
LOG_LEVEL=info
